generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id @default(autoincrement())
  name String
  email String @unique
  password String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("users")
}





model teachers{
techerName String 
teacherAge Int 
teacherMobile BigInt 
teacherScore Int 
teacherId Int @id
}






model Students{
id Int @id
firstName String 
lastName String 
age Int 
joinedDate DateTime @default(now())
}


